{
  "name": "uilib",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "main": "build/index.js",
  "module": "build-es/index.js",
  "dependencies": {
    "copy-to-clipboard": "^3.0.8",
    "emotion": "^9.2.12",
    "emotion-theming": "^9.2.9",
    "immutability-helper": "^2.8.1",
    "lodash": "^4.17.11",
    "polished": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-scripts": "2.1.0",
    "recompact": "^3.4.0",
    "recompose": "^0.30.0"
  },
  "scripts": {
    "start": "start-storybook -p 9001",
    "build": "yarn build-es && yarn build-cjs",
    "build-cjs": "babel src -d build --source-maps --ignore src/**/*.test.js,src/**/*.story.js --plugins=babel-plugin-transform-es2015-modules-commonjs",
    "build-es": "babel src -d build-es --ignore src/**/*.test.js,src/**/*.story.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o dist/storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.0",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-info": "^5.0.0",
    "@storybook/addon-jest": "^5.0.0",
    "@storybook/addon-knobs": "^5.0.0",
    "@storybook/addon-links": "^5.0.0",
    "@storybook/addon-notes": "^5.0.0",
    "@storybook/addon-options": "^5.0.0",
    "@storybook/addon-storysource": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-macros": "^2.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "docz": "^0.12.9",
    "docz-plugin-css": "^0.11.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "jest-emotion": "^9.2.11",
    "jest-enzyme": "^7.0.0",
    "react-test-renderer": "^16.5.2",
    "watch": "^1.0.2",
    "webpack": "^4.22.0",
